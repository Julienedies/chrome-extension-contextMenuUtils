{
  "manifest_version": 2,
  "name": "jhandy",
  "description": "自用工具集合",
  "version": "1.1",
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "img/icon-bitty.png",
    "48": "img/icon-small.png",
    "128": "img/icon-large.png"
  },
  "browser_action": {
    "default_icon": "img/icon-large.png",
    "default_popup": "options.html",
    "default_title": "自定义页面"
  },
  "permissions": [
    "tabs",
    "notifications",
    "contextMenus",
    "downloads",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "\u003Call_urls\u003E",
    "http://*/*",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "js/libs/chrome.js",
      "js/bg/contextMenus.js",
      "js/bg/webRequest.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["http://es6.ruanyifeng.com/*"],
      "css": ["css/cs/es6.css"]
    },
    {
      "matches": ["http://www.maofou.com/*"],
      "css": ["css/cs/maofou.css"]
    },
    {
      "matches": ["https://www.taoguba.com.cn/*"],
      "css": ["css/cs/taoguba.css"]
    },
    {
      "matches": ["https://www.cailianpress.com/*", "http://www.yuncaijing.com/insider/simple.html"],
      "css": ["css/cs/cailianpress.css"],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/cailianpress.js"
      ]
    },
    {
      "matches": ["*://www3.55188.com/*"],
      "css": ["css/cs/960.css"]
    },
    {
      "matches": ["http://taobao.cjcp.com.cn/*"],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/ng.js"
      ]
    },
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": ["https://www.zhihu.com/*"],
      "js": [
        "js/cs/back-top.js"
      ]
    },
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/libs/chrome.js"
      ]
    },
    {
      "matches": [
        "http://www.bbc.com/*"
      ],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/open-link-in-new-window.js"
      ]
    },
    {
      "matches": [
        "http://www.yuncaijing.com/quote/*"
      ],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/ycj.js"
      ]
    },
    {
      "matches": [
        "http://localhost:3000/*"
      ],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/jhandy.js"
      ]
    },
    {
      "matches": [
        "https://www.taoguba.com.cn/*"
      ],
      "js": [
        "js/data/stock-list.js",
        "js/vendor/jquery.min.js",
        "js/cs/back-top.js",
        "js/cs/reset-stock-link.js"
      ]
    },
    {
      "matches": [
        "https://gupiao.baidu.com/stock/*",
        "http://finance.sina.com.cn/realstock/company/*"
      ],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/fix-stock-k-line.js"
      ]
    },
    {
      "matches": [
        "https://xueqiu.com/S/*"
      ],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/xueqiu.js"
      ]
    },
    {
      "matches": [
        "https://www.iwencai.com/data-robot/*"
      ],
      "css": ["css/cs/iwencai.css"],
      "js": [
        "js/vendor/jquery.min.js",
        "js/cs/iwencai.js"
      ]
    },
    {
      "matches": ["http://basic.10jqka.com.cn/*/*"],
      "exclude_matches": [
      ],
      "css": ["css/cs/10jqka.css"],
      "js": [
        "js/vendor/jquery.min.js",
        "js/data/T.js",
        "js/cs/10jqka.js"
      ]
    }
  ]
}
