{
  "manifest_version": 2,
  "name": "contextMenuUtils",
  "description": "custom page",
  "version": "1.0",
  "icons": {
    "16": "img/icon-bitty.png",
    "48": "img/icon-small.png",
    "128": "img/icon-large.png"
  },
  "browser_action": {
    "default_icon": "img/icon-large.png",
    "default_popup": "popup.html",
    "default_title": "自定义页面"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "downloads",
    "storage",
    "\u003Call_urls\u003E",
    "http://*/*",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "js/libs/jquery.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["http://taobao.cjcp.com.cn/*"],
      "js": [
        "js/libs/jquery.min.js",
        "js/cs/ng.js"
      ]
    },
    {
      "matches": ["https://www.taoguba.com.cn/*", "http://chinadigitaltimes.net/*", "https://www.amazon.cn/*", "https://www.futunn.com/*"],
      "exclude_matches": ["https://www.zhihu.com/*"],
      "js": [
        "js/cs/back-top.js"
      ]
    },
    {
      "matches": ["http://www.bbc.com/*", "http://chinadigitaltimes.net/*","https://www.5zipai.com/*"],
      "js": [
        "js/libs/jquery.min.js",
        "js/cs/open-link-in-new-window.js"
      ]
    },
    {
      "matches": ["https://www.taoguba.com.cn/*"],
      "js": [
        "js/libs/jquery.min.js",
        "js/cs/reset-stock-link.js"
      ]
    },
    {
      "matches": ["https://gupiao.baidu.com/stock/*"],
      "js": [
        "js/libs/jquery.min.js",
        "js/cs/fix-baidu-stock.js"
      ]
    },
    {
      "matches": ["https://www.cailianpress.com/"],
      "css":["css/cs/cailianpress.css"]
    }
  ]

}